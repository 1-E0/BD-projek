<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="600.0" prefWidth="800.0" spacing="15.0" styleClass="root" stylesheets="@../styles/style.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.bd.controller.RiwayatPesananController">
    <children>
        <Label fx:id="lblTitle" styleClass="title-label" text="Riwayat Pesanan Anda" />
        <ListView fx:id="pesananListView" VBox.vgrow="ALWAYS" />
        <Button mnemonicParsing="false" onAction="#handleKembali" styleClass="delete-button" text="Kembali ke Dashboard" />
    </children>
    <padding>
        <Insets bottom="25.0" left="25.0" right="25.0" top="25.0" />
    </padding>
</VBox>